    <BUILD_ALIAS>Final</BUILD_ALIAS>
  </properties>

  <modules>
    <module>org.jboss.tools.locus.jcip.annotations</module>
    <module>org.jboss.tools.locus.easytesting.fest-assert</module>
    <module>org.jboss.tools.locus.easytesting.fest-util</module>
    <module>org.jboss.tools.locus.sf.saxon</module>
    <module>org.jboss.tools.locus.mockito</module>
    <module>org.jboss.tools.locus.dmr</module>
    <module>org.jboss.tools.locus.jsonschema2pojo</module>
    <module>org.jboss.tools.locus.apache.directory.bouncycastle.stub</module>
    <module>org.jboss.tools.locus.jaxb-core</module>
    <module>org.jboss.tools.locus.jaxb-impl</module>
    <module>org.jboss.tools.locus.jaxb-xjc</module>
  </modules>
</project>
  
